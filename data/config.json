  "selected_attributes": {
    "year": true,
    "month": true,
    "day": true,
    "day_of_week": true,
    "hour_of_day": true,
    "minute_of_hour": true,
    "prod_p": true,
    "prod_q": true,
    "prod_v": true,
    "load_p": true,
    "load_q": true,
    "load_v": true,
    "p_or": true,
    "q_or": true,
    "v_or": true,
    "a_or": true,
    "p_ex": true,
    "q_ex": true,
    "v_ex": true,
    "a_ex": true,
    "rho": true,
    "topo_vect": true,
    "line_status": true,
    "timestep_overflow": true,
    "time_before_cooldown_line": true,
    "time_before_cooldown_sub": true,
    "time_next_maintenance": true,
    "duration_next_maintenance": true,
    "actual_dispatch": true,
    "target_dispatch": true
  }
