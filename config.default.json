{
  "env": "l2rpn_neurips_2020_track2_small",
  "seed": 2020,
  "use_gpu": true,
  "check_point_folder": "check_points",
  "check_point_episodes": 10,
  "neptune_enabled": false,
  "neptune_api_token": "your Neptune api token here if enable Neptune",
  "neptune_project_name": "your Neptune project name here if enable Neptune",
  "hyper_parameters": {
    "train_num_episodes": 1000,
    "min_steps_before_learning": 512,
    "num_frames": 4,
    "batch_size": 256,
    "learning_rate": 0.005,
    "gradient_clipping_norm": 5.0,
    "discount_rate": 0.99,
    "epsilon_decay_rate_denominator": 1.0,
    "normalise_rewards": true,
    "exploration_worker_difference": 2.0,
    "clip_rewards": false,
    "Actor": {
      "learning_rate": 0.0003,
      "linear_hidden_units": [
        256,
        256
      ],
      "final_layer_activation": "Softmax",
      "batch_norm": false,
      "tau": 0.005,
      "gradient_clipping_norm": 5,
      "initializer": "Xavier"
    },
    "Critic": {
      "learning_rate": 0.0003,
      "linear_hidden_units": [
        256,
        256
      ],
      "final_layer_activation": "None",
      "batch_norm": false,
      "buffer_size": 1000000,
      "tau": 0.005,
      "gradient_clipping_norm": 5,
      "initializer": "Xavier"
    },
    "mu": 0.0,
    "theta": 0.15,
    "sigma": 0.25,
    "action_noise_std": 0.2,
    "action_noise_clipping_range": 0.5,
    "update_every_n_steps": 1,
    "learning_updates_per_learning_session": 1,
    "automatically_tune_entropy_hyper_parameter": true,
    "entropy_term_weight": "None",
    "add_extra_noise": false,
    "do_evaluation_iterations": true,
    "training_episodes_per_eval_episode": 20,
    "selected_attributes": [
      "day_of_week",
      "hour_of_day",
      "minute_of_hour",
      "prod_p",
      "prod_v",
      "load_p",
      "load_q",
      "actual_dispatch",
      "target_dispatch",
      "topo_vect",
      "time_before_cooldown_line",
      "time_before_cooldown_sub",
      "rho",
      "timestep_overflow",
      "line_status"
    ]
  }
}