{
  "env": "l2rpn_case14_sandbox",
  "seed": 2020,
  "use_gpu": true,
  "check_point_folder": "check_points",
  "check_point_episodes": 100,
  "neptune_enabled": false,
  "neptune_api_token": "Your Neptune API token here if neptune enabled",
  "neptune_project_name": "Your Neptune API project here if neptune enabled",
  "hyper_parameters": {
    "train_num_episodes": 20000,
    "min_steps_before_learning": 10000,
    "num_stack_frames": 4,
    "batch_size": 64,
    "discount_rate": 0.99,
    "buffer_size": 1000000,
    "Actor": {
      "learning_rate": 0.0003
    },
    "Critic": {
      "learning_rate": 0.0003,
      "tau": 0.005
    },
    "update_every_n_steps": 1,
    "learning_updates_per_learning_session": 1,
    "automatically_tune_entropy_hyper_parameter": true,
    "entropy_term_weight": "None",
    "do_evaluation_iterations": true,
    "training_episodes_per_eval_episode": 10,
    "selected_attributes": {
      "year": true,
      "month": true,
      "day": true,
      "day_of_week": true,
      "hour_of_day": true,
      "minute_of_hour": true,
      "prod_p": true,
      "prod_q": true,
      "prod_v": true,
      "load_p": true,
      "load_q": true,
      "load_v": true,
      "p_or": true,
      "q_or": true,
      "v_or": true,
      "a_or": true,
      "p_ex": true,
      "q_ex": true,
      "v_ex": true,
      "a_ex": true,
      "rho": true,
      "topo_vect": true,
      "line_status": true,
      "timestep_overflow": true,
      "time_before_cooldown_line": true,
      "time_before_cooldown_sub": true,
      "time_next_maintenance": true,
      "duration_next_maintenance": true,
      "actual_dispatch": true,
      "target_dispatch": true
    }
  }
}
